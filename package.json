{
  "name": "codius-manifest",
  "version": "2.0.2",
  "description": "A module for validating Codius manifests",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && nyc mocha",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codius/manifest.git"
  },
  "keywords": [
    "codius",
    "manifest"
  ],
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/codius/manifest/issues"
  },
  "homepage": "https://github.com/codius/manifest#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.2.0",
    "nyc": "^11.8.0",
    "sinon": "^5.0.10"
  },
  "dependencies": {
    "canonical-json": "0.0.4",
    "debug": "^3.1.0",
    "fs-extra": "^6.0.1",
    "jsen": "^0.6.6",
    "thirty-two": "^1.0.2"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 75,
    "branches": 75,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true
  }
}
